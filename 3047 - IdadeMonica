#include<stdio.h>
int main(){

    int M, A = 1, B = 1, X;

        scanf("%d", &M);
        scanf("%d", &A);
        scanf("%d", &B);

         X = M - A - B;

        if(A > B && A > X && A != B)
        {
          printf("%d\n", A);
        }
        else if(B > A && B > X && B != A )
        {
            printf("%d\n", B);
        }
        else
            printf("%d\n", X);


    return 0;
}
